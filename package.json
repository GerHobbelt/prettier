{
  "name": "@gerhobbelt/prettier",
  "version": "1.8.2-4",
  "description": "Prettier is an opinionated code formatter",
  "bin": {
    "prettier": "./bin/prettier.js"
  },
  "repository": "prettier/prettier",
  "homepage": "https://prettier.io",
  "author": "James Long",
  "license": "MIT",
  "main": "./index.js",
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "babel-code-frame": "7.0.0-alpha.12",
    "babylon": "7.0.0-beta.31",
    "camelcase": "4.1.0",
    "chalk": "2.3.0",
    "cjk-regex": "1.0.2",
    "cosmiconfig": "3.1.0",
    "dashify": "0.2.2",
    "diff": "3.4.0",
    "emoji-regex": "6.5.1",
    "escape-string-regexp": "1.0.5",
    "esutils": "2.0.2",
    "flow-parser": "0.59.0",
    "get-stream": "3.0.0",
    "globby": "7.1.1",
    "graphql": "0.11.7",
    "ignore": "3.3.7",
    "jest-docblock": "21.3.0-beta.7",
    "jest-validate": "21.2.1",
    "leven": "2.1.0",
    "mem": "3.0.0",
    "minimatch": "3.0.4",
    "minimist": "1.2.0",
    "parse5": "3.0.3",
    "postcss-less": "1.1.3",
    "postcss-media-query-parser": "0.2.3",
    "postcss-scss": "1.0.2",
    "postcss-selector-parser": "3.1.1",
    "postcss-values-parser": "1.3.1",
    "remark-frontmatter": "1.1.0",
    "remark-parse": "9.0.0",
    "semver": "5.4.1",
    "string-width": "2.1.1",
    "typescript": "2.6.2",
    "typescript-eslint-parser": "9.0.1",
    "unicode-regex": "1.0.1",
    "unified": "6.1.6"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "codecov": "3.0.0",
    "cross-env": "5.1.1",
    "eslint": "4.11.0",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.5.1",
    "jest": "21.2.1",
    "mkdirp": "0.5.1",
    "prettier": "1.8.2",
    "prettylint": "1.0.0",
    "rimraf": "2.6.2",
    "rollup": "0.52.0",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.1.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-replace": "2.0.0",
    "shelljs": "0.7.8",
    "strip-ansi": "4.0.0",
    "sw-toolbox": "3.6.0",
    "uglify-es": "3.2.0",
    "webpack": "3.8.1"
  },
  "scripts": {
    "origPrepublishOnly": "echo \"Error: must publish from dist/\" && exit 1",
    "origPrepare-release": "yarn && yarn build && yarn test:dist",
    "test": "jest",
    "test:dist": "cross-env NODE_ENV=production yarn test",
    "posttest": "npm run test-tabs && npm run test-tabs-inv",
    "test-tabs": "node ./bin/prettier.js --use-tabs --bracket-spacing --trailing-comma array,object -- ./bin/prettier.js > ./bin/prettier-with-tabs.js",
    "test-tabs-inv": "node ./bin/prettier-with-tabs.js -- ./bin/prettier-with-tabs.js > ./bin/prettier-spaces.js",
    "test-integration": "jest tests_integration",
    "update-snapshots": "jest --updateSnapshot",
    "lint": "cross-env EFF_NO_LINK_RULES=true eslint . --format node_modules/eslint-friendly-formatter",
    "lint-docs": "prettylint {.,docs,website}/*.md",
    "build": "node ./scripts/build/build.js",
    "build-docs": "node ./scripts/build/build-docs.js",
    "check-deps": "node ./scripts/check-deps.js",
    "pub": "cd dist && npm publish --access public"
  }
}
